<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <%if(orders.length <= 0) { %>
                <h1>Nothing there!</h1>
            <%} else {%>
                <ul>
                    <%orders.forEach(o =>{ %>
                    <li>
                        <h1># <%= o.id%></h1>
                        <ul>
                            <% o.products.forEach( product => {%>
                                <li><%= product.title%>   (<%= product.orderItem.quantity %>)</li>
                            <%}); %>    
                        </ul>
                    </li>
                    <% }); %>
                </ul>
            <% } %>    
        </main>
        <%- include('../includes/end.ejs') %>